<?xml version="1.0"?>
<extension key="de.systopia.xportx" type="module">
  <file>xportx</file>
  <name>Extensible Export</name>
  <description>SYSTOPIA's modular export extension</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.xportx</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.xportx</url>
    <url desc="Support">https://github.com/systopia/de.systopia.xportx/issues</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2023-03-28</releaseDate>
  <version>0.9-alpha1</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.27</ver>
  </compatibility>
  <comments/>
  <civix>
    <namespace>CRM/xportx</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
</extension>
